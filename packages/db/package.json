{
  "name": "@prepi/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "with-env": "dotenv -e ../../.env --",
    "clean": "rm -rf .turbo node_modules",
    "dev": "pnpm with-env pnpm prisma studio --port 5556",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:migrate": "pnpm with-env prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1"
  },
  "devDependencies": {
    "@prepi/eslint-config": "workspace:*",
    "@prepi/typescript-config": "workspace:*",
    "dotenv-cli": "^6.0.0",
    "prisma": "^5.19.1",
    "typescript": "^4.9.3"
  }
}
